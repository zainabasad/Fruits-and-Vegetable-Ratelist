    <link href="~/Content/bootstrap/js/owl-carousel/owl.carousel.css" rel="stylesheet">
    <link rel="stylesheet" href="css/styles.css" />
    <!-- Font Awesome -->
    <link href="~/Content/bootstrap/font/css/font-awesome.min.css" rel="stylesheet">




</head>
<body style="background-color:peachpuff">

    <script src="~/Content/bootstrap/js/modernizr-latest.js"></script>
    <script src="~/Content/bootstrap/js/jquery-1.8.2.min.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/jquery.isotope.min.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/fancybox/jquery.fancybox.pack.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/jquery.nav.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/jquery.cslider.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/custom.js" type="text/javascript"></script>
    <script src="~/Content/bootstrap/js/owl-carousel/owl.carousel.js"></script>
    <!-- <h1 class=" text-primary" style="font-family:'Comic Sans MS';font-size:40px;padding-left:300px;padding-bottom:50px;padding-top:10px"> Fruits and Vegetables Rates  </h1> -->
    <header class="header">
        <nav class="navbar navbar-inverse " role="navigation">
            <div class=" container-fluid">

                <ul class="nav navbar-nav" style="padding-right:50px">
                    <li class=" text-success" style="font-family: 'Comic Sans MS'; font-size: 20px;padding-top:30px"> Fruits and Vegetables Rates  </li>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contacts", "Home")</li>
                    <li> <a href="~/Home/Login"> Login <span class="glyphicon glyphicon-log-in"></span></a> </li>
                    <li> <a href="~/Home/SignUp"> Sign Up <span class="glyphicon glyphicon-user"></span></a> </li>
                    <!-- <li> <a href="HomeLogin"> Login <span class="glyphicon glyphicon-log-in"></span></a> </li>-->
                </ul>
            </div>
        </nav>
    </header>


    


    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)

